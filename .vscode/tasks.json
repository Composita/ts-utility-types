{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "yarn: build",
			"dependsOn": [ "yarn: install" ],
			"command": "yarn",
			"args": [
				"build"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"type": "shell",
			"label": "yarn: install",
			"command": "yarn",
			"args": [
				"install"
			],
			"group": "build"
		}
	]
}
